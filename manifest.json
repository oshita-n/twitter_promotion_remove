{
    "name": "twitter-promotion-remove",
    "author": "Noriaki Oshita",
    "description": "remove twitter promotion.",
    "version": "1.0.0",
    "manifest_version": 2,
    "web_accessible_resources": ["*"],
    "permissions": ["storage"],
    "content_scripts": [
      {
      "matches": ["https://twitter.com/*"],
      "css": ["twitter/style.css"],"js": ["twitter/script.js"]
      }
    ]
  }